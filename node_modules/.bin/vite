#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/f/从零到一组件库封装/Vue3+TS+vite/node_modules/.pnpm/vite@6.0.7_@types+node@22.10.5_less@4.2.1/node_modules/vite/bin/node_modules:/mnt/f/从零到一组件库封装/Vue3+TS+vite/node_modules/.pnpm/vite@6.0.7_@types+node@22.10.5_less@4.2.1/node_modules/vite/node_modules:/mnt/f/从零到一组件库封装/Vue3+TS+vite/node_modules/.pnpm/vite@6.0.7_@types+node@22.10.5_less@4.2.1/node_modules:/mnt/f/从零到一组件库封装/Vue3+TS+vite/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/f/从零到一组件库封装/Vue3+TS+vite/node_modules/.pnpm/vite@6.0.7_@types+node@22.10.5_less@4.2.1/node_modules/vite/bin/node_modules:/mnt/f/从零到一组件库封装/Vue3+TS+vite/node_modules/.pnpm/vite@6.0.7_@types+node@22.10.5_less@4.2.1/node_modules/vite/node_modules:/mnt/f/从零到一组件库封装/Vue3+TS+vite/node_modules/.pnpm/vite@6.0.7_@types+node@22.10.5_less@4.2.1/node_modules:/mnt/f/从零到一组件库封装/Vue3+TS+vite/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
